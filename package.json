{
  "name": "syncrow",
  "version": "0.0.2",
  "description": "Easy directory synchronization over the network",
  "main": "index.js",
  "scripts": {
    "test": "mocha --require source-map-support/register --recursive build/test",
    "compile": "tsc || true",
    "build": "npm run clear && npm run compile",
    "clear": "rm -Rf build",
    "benchmark": "./housekeeping/benchmarks.sh",
    "benchmark:short": "./housekeeping/benchmark-short.sh",
    "lint": "tslint `find src -name '*.ts*'`",
    "clear-maps": "find build -name \"*.js.map\" -type f -delete",
    "prepare": "npm run build && npm run clear-maps && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jan-osch/syncrow.git"
  },
  "author": "jan-osch",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jan-osch/syncrow/issues"
  },
  "bin": {
    "syncrow": "bin/syncrow"
  },
  "homepage": "https://github.com/jan-osch/syncrow#readme",
  "dependencies": {
    "async": "^2.0.0-rc.3",
    "chalk": "^1.1.3",
    "chokidar": "^1.5.2",
    "commander": "^2.9.0",
    "debug": "^2.2.0",
    "ignore": "^3.1.5",
    "inquirer": "^1.1.2",
    "lodash": "^4.13.1",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.5.2",
    "upath": "^0.1.7"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "git-rev-sync": "^1.6.0",
    "mocha": "^2.5.3",
    "rewire": "^2.5.1",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0",
    "source-map-support": "^0.4.2",
    "tslint": "^3.15.1",
    "typescript": "^1.8.10"
  }
}
